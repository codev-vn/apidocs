{
    "name": "codev/apidocs",
    "description": "Laravel API Documentation Generator",
    "authors": [
        {
            "name": "CoDev Service Co.,Ltd",
            "email": "hello@codev.vn"
        }
    ],
    "homepage": "https://github.com/nhiepphong/apidocs",
    "license": "MIT",
    "require": {
        "illuminate/support": "^8.41",
        "php": "^7.3",
        "phpdocumentor/reflection-docblock": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Codev\\Apidocs\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Codev\\Apidocs\\ApidocsServiceProvider"
            ]
        }
    }
}
